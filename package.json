{
  "name": "inklip",
  "productName": "影氪",
  "version": "0.0.51",
  "description": "智能视频剪辑工具",
  "main": "./dist/main/index.js",
  "author": "caleb",
  "homepage": "https://electron-vite.org",
  "scripts": {
    "start": "electron-forge start",
    "dev": "electron-vite dev --outDir=dist",
    "debug": "cross-env DEBUG_MODE=true DEBUG_PORT=12698 electron-vite dev --outDir=dist",
    "build": "electron-vite build --outDir=dist",
    "package": "pnpm build && electron-forge package",
    "make": "pnpm build && electron-forge make",
    "make:win": "pnpm build && electron-forge make --platform win32 --arch x64",
    "release": "node scripts/release.js",
    "release:patch": "node scripts/release.js patch",
    "release:minor": "node scripts/release.js minor",
    "release:major": "node scripts/release.js major"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/utils": "^4.0.0",
    "@vicons/ionicons5": "^0.13.0",
    "axios": "^1.13.5",
    "electron-updater": "^6.8.3",
    "naive-ui": "^2.43.2",
    "pinia": "^3.0.4",
    "vue-router": "^5.0.3"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.11.1",
    "@electron-forge/maker-dmg": "^7.11.1",
    "@electron-forge/maker-zip": "^7.11.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.11.1",
    "@electron-forge/plugin-fuses": "^7.11.1",
    "@electron-toolkit/eslint-config-prettier": "3.0.0",
    "@electron-toolkit/eslint-config-ts": "^3.1.0",
    "@electron-toolkit/tsconfig": "^2.0.0",
    "@electron/fuses": "^2.0.0",
    "@types/node": "^25.3.0",
    "@vitejs/plugin-vue": "^6.0.4",
    "cross-env": "^10.1.0",
    "electron": "^40.6.1",
    "electron-vite": "^5.0.0",
    "eslint": "^10.0.2",
    "eslint-plugin-vue": "^10.8.0",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vue": "^3.5.29",
    "vue-eslint-parser": "^10.4.0",
    "vue-tsc": "^3.2.5"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "electron",
      "esbuild",
      "macos-alias",
      "fs-xattr",
      "electron-winstaller"
    ],
    "patchedDependencies": {
      "cross-zip@4.0.1": "patches/cross-zip@4.0.1.patch"
    }
  }
}
